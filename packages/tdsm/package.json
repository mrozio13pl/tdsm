{
    "name": "tdsm",
    "version": "0.0.0-alpha",
    "description": "Automatically installs typescript declarations for your dependencies.",
    "main": "lib/index.js",
    "bin": {
        "tdsm": "bin/tdsm"
    },
    "repository": "mrozio13pl/tdsm",
    "author": {
        "name": "mrozio13pl"
    },
    "license": "MIT",
    "scripts": {
        "build": "run-s build:** -l",
        "build:pretty": "prettier ./src/ --write",
        "build:linter": "eslint src",
        "build:format": "eslint src --fix",
        "build:clean": "del-cli lib",
        "build:bundle": "tsup",
        "dev": "tsc-watch src/index.ts --onSuccess \"npm run build\" --outDir ./lib"
    },
    "keywords": [
        "types"
    ],
    "files": [
        "bin",
        "lib"
    ],
    "dependencies": {
        "@tdsm/cache": "*",
        "@tdsm/core": "*",
        "@tdsm/logger": "*",
        "@tdsm/types": "*",
        "ansis": "^1.5.5",
        "defu": "^6.1.2",
        "nanospinner": "^1.1.0",
        "p-map-series": "^2.1.0",
        "resolve-version": "^1.0.0",
        "sade": "^1.8.1",
        "semver": "^6.3.1"
    },
    "devDependencies": {
        "@swc/core": "^1.3.83",
        "@types/node": "^20.6.0",
        "@types/semver": "^7.5.0",
        "@typescript-eslint/eslint-plugin": "^6.4.0",
        "@typescript-eslint/parser": "^6.4.0",
        "del-cli": "^5.1.0",
        "eslint": "^8.47.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^3.0.2",
        "tsc-watch": "^6.0.4",
        "tsup": "^7.2.0",
        "typescript": "^5.1.6"
    },
    "publishConfig": {
        "access": "public"
    },
    "gitHead": "f84ae1c785425d520abc64c8024eed4b01919ff7"
}
